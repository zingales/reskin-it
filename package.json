{
  "name": "reskin-it",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev:full": "concurrently \"npm run server\" \"cd client && npm run dev\"",
    "build": "npm run build:server && npm run build:client",
    "build:server": "tsc -b",
    "build:client": "cd client && npm run build",
    "start": "npx tsx server/index.ts",
    "server": "npx tsx server/index.ts",
    "seed": "npx tsx prisma/seed.ts",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:push:sqlite": "prisma db push --schema=./prisma/schema.sqlite.prisma",
    "db:generate:sqlite": "prisma generate --schema=./prisma/schema.sqlite.prisma",
    "db:deploy": "prisma db push && npm run seed",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:migrate:dev": "prisma migrate dev",
    "db:migrate:dev:sqlite": "prisma migrate dev --schema=./prisma/schema.sqlite.prisma",
    "db:migrate:reset": "prisma migrate reset",
    "db:migrate:reset:sqlite": "prisma migrate reset --schema=./prisma/schema.sqlite.prisma",
    "migrate": "node scripts/migrate.js"
  },
  "dependencies": {
    "@prisma/client": "^6.13.0",
    "@vercel/node": "^5.3.12",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "prisma": "^6.13.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.10",
    "concurrently": "^9.2.0",
    "tsx": "^4.19.2",
    "typescript": "~5.8.3"
  }
}
